                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
      [IdKlient] as IdKlientT
      ,[Imie]
      ,[Nazwisko]
      ,[DataUrodzenia]
      ,[Pesel]
      ,[AdresMailowy]
      ,[NumerTelefonu]
      ,k.[IdAdres] as IdAdresT
      ,[Ulica]
      ,[Miasto]
      ,[KodPocztowy]
      ,[NumerLokalu]

  FROM [dbo].[Klienci] k inner join [dbo].[Adresy] a on k.IdAdres = a.IdAdres
    where k.[DataModyfikacji] &gt;= dateadd(day,-1,cast(CURRENT_TIMESTAMP as date))
union
SELECT
      [IdKlient] as IdKlientT
      ,[Imie]
      ,[Nazwisko]
      ,[DataUrodzenia]
      ,[Pesel]
      ,[AdresMailowy]
      ,[NumerTelefonu]
